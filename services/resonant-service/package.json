{
  "name": "resonant-service",
  "version": "1.0.0",
  "description": "Minimal Express service with CI, Docker, Compose",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "vitest",
    "prepare": "npm run build",
    "docker:build": "docker build -t resonant-service .",
    "docker:run": "docker run -p 3000:3000 resonant-service",
    "ci:test": "npm run build && npm run test"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pino": "^8.21.0"
  },
  "devDependencies": {
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "vitest": "^0.35.0",
    "@types/node": "^20.3.1",
    "@types/express": "^4.17.20",
    "pino-pretty": "^11.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SamuelJacksonGrim/resonant-service.git"
  },
  "author": "Samuel Jackson Grim",
  "license": "MIT"
}
